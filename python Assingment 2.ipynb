     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the value= ineuron\n",
      "norueni\n"
     ]
    }
   ],
   "source": [
    "txt=input(\"Enter the value= \")\n",
    "txt=txt[::-1]\n",
    "print(txt)"
   ]
  }
